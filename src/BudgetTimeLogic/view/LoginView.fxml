<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.ColorAdjust?>
<?import javafx.scene.effect.SepiaTone?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="BudgetTimeLogic.view.loginControler">
   <children>
      <PasswordField fx:id="pass" accessibleRole="TEXT_FIELD" layoutX="110.0" layoutY="233.0" promptText="Password" />
      <TextField fx:id="user" layoutX="110.0" layoutY="200.0" promptText="User Name" />
      <Text fill="#2f2cc6" layoutX="89.0" layoutY="110.0" stroke="BLACK" strokeLineCap="ROUND" strokeType="OUTSIDE" strokeWidth="0.0" text="BudgetTIme.IO" wrappingWidth="222.22314453125">
         <font>
            <Font name="Impact" size="37.0" />
         </font>
      </Text>
      <Button fx:id="loginButton" contentDisplay="CENTER" layoutX="106.0" layoutY="304.0" mnemonicParsing="false" onAction="#handleLogin" prefHeight="29.0" prefWidth="74.0" text="Login">
         <font>
            <Font name="Avenir Next Medium" size="13.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <ColorAdjust />
         </effect>
      </Button>
      <Button fx:id="cancelButton" layoutX="207.0" layoutY="304.0" mnemonicParsing="false" onAction="#handleCancle" prefHeight="29.0" prefWidth="74.0" text="Cancel">
         <font>
            <Font name="Avenir Next Medium" size="13.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <ColorAdjust />
         </effect>
      </Button>
      <DialogPane fx:id="pleaseLog" contentText="Please login" layoutX="109.0" layoutY="143.0" prefHeight="57.0" prefWidth="167.0" />
   </children>
   <effect>
      <SepiaTone level="0.0" />
   </effect>
</AnchorPane>
